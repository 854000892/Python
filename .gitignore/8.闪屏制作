i='吃粑粑'
print(i,'あ'*27)
print('い',i,'い'*26)
print('う'*2,i,'う'*25)
print('え'*3,i,'え'*24)
print('お'*4,i,'お'*23)
print('か'*5,i,'か'*22)
print('き'*6,i,'き'*21)
print('く'*7,i,'く'*20)
print('け'*8,i,'け'*19)
print('こ'*9,i,'こ'*18)
print('さ'*10,i,'さ'*17)
print('し'*11,i,'し'*16)
print('す'*12,i,'す'*15)
print('せ'*13,i,'せ'*14)
print('そ'*14,i,'そ'*13)
print('た'*15,i,'た'*12)
print('ち'*16,i,'ち'*11)
print('て'*17,i,'て'*10)
print('と'*18,i,'と'*9)
print('な'*19,i,'な'*8)
print('に'*20,i,'に'*7)
print('ぬ'*21,i,'ぬ'*6)
print('ね'*22,i,'ね'*5)
print('は'*23,i,'は'*4)
print('ひ'*24,i,'ま'*3)
print('ふ'*25,i,'ふ'*2)
print('ほ'*26,i,'ほ'*1)
print('み'*27,i)
print('你会发现，除了吃粑粑你啥都不会')
